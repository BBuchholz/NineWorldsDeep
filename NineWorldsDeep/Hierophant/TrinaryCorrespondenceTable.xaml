<UserControl x:Class="NineWorldsDeep.Hierophant.TrinaryCorrespondenceTable"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NineWorldsDeep.Hierophant"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl TabStripPlacement="Bottom">
            <TabItem Header="Mothers">
                <DataGrid x:Name="dgridMothers"/>
            </TabItem>
            <TabItem Header="Doubles">
                <DataGrid x:Name="dgridDoubles"/>
            </TabItem>
            <TabItem Header="Elementals">
                <DataGrid x:Name="dgridElementals"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
